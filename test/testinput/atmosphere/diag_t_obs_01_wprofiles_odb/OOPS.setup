#!/bin/sh
# ODB_LD=""; export ODB_LD
# ODB_LIBS=""; export ODB_LIBS
ioassign -s -a \$ODB_SRCPATH_OOPS/OOPS.dd OOPS
ioassign -s -a \$ODB_SRCPATH_OOPS/OOPS.sch OOPS.sch
ioassign -s -r 1m -w 1m -a \$ODB_SRCPATH_OOPS/OOPS.iomap OOPS.iomap
ioassign -s -a \$ODB_SRCPATH_OOPS/OOPS.flags OOPS.flags
ioassign -s -r 1m -w 1m -a \$ODB_SRCPATH_OOPS/dca/desc.dca OOPS.desc.dca
ioassign -s -r 1m -w 1m -a \$ODB_SRCPATH_OOPS/dca/hdr.dca OOPS.hdr.dca
ioassign -s -r 1m -w 1m -a \$ODB_SRCPATH_OOPS/dca/body.dca OOPS.body.dca
ioassign -s -n 32 -r 1m -w 1m -a \$ODB_DATAPATH_OOPS/%d/desc OOPS.desc.%d
ioassign -s -n 32 -r 1m -w 1m -a \$ODB_DATAPATH_OOPS/%d/hdr OOPS.hdr.%d
ioassign -s -n 32 -r 1m -w 1m -a \$ODB_DATAPATH_OOPS/%d/body OOPS.body.%d
